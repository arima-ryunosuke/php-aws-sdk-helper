{
  "config": {
    "platform": {
      "php": "8.2"
    }
  },
  "name": "ryunosuke/aws-sdk-helper",
  "type": "library",
  "description": "aws-sdk-php helper",
  "license": "MIT",
  "authors": [
    {
      "name": "ryunosuke",
      "email": "ryunosuke.arima@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.2"
  },
  "require-dev": {
    "aws/aws-sdk-php": "3.*",
    "phpunit/phpunit": "9.*",
    "ryunosuke/phpunit-extension": "3.*"
  },
  "autoload": {
    "psr-4": {
      "ryunosuke\\Aws\\Helper\\": "src/Aws/Helper"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ryunosuke\\Test\\": "tests/Test"
    }
  },
  "bin": [
    "bin/build-client"
  ],
  "scripts": {
    "build": [
      "@test"
    ],
    "test": [
      "php vendor/bin/phpunit --configuration=tests/phpunit.xml"
    ]
  }
}
